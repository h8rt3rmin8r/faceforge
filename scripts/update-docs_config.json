      "html": "desktop/README.html",
      "pdf": "desktop/README.pdf"
    },
    {
      "source": "scripts/README.md",
      "html": "scripts/README.html",
      "pdf": "scripts/README.pdf"
    },
    {
      "source": "docs/FaceForge Core - Project Design Specification.md",
      "html": "docs/FaceForge Core - Project Design Specification.html",
      "pdf": "docs/FaceForge Core - Project Design Specification.pdf"
